{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "index.js"
        }
    ],
    "functions": {
        "api/scrape.js": {
          "runtime": "nodejs14.x",
          "memory": 1024
        }
      }
} 